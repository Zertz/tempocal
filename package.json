{
  "name": "tempocal",
  "version": "0.1.0",
  "description": "Highly flexible building blocks to build calendars with Temporal API",
  "repository": "https://github.com/Zertz/tempocal",
  "author": "Pier-Luc Gendreau",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "parcel build packages/*",
    "lint": "eslint . --ext ts,tsx",
